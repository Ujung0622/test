<script>
function divid(alpha, beta){
	if(beta ==0) {
		throw 'DivideByZeroException';
	} else {
		return alpha /beta;
	}
}

try {
	divide(10,0);
	
} catch(exeption){
	alert("CATCH");
}
</script>

</script>
